<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.acadview.www.aq">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_appicon_round"
        android:supportsRtl="true"
        android:theme="@style/SplashTheme">
        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".AlarmReciever" />

        <activity
            android:name=".Catalog"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MyProfile"
            android:screenOrientation="portrait" />
        <activity
            android:name=".QuizStart"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Playing"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Done"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ScoreDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Password_change"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AdminView"
            android:screenOrientation="portrait" />
        <activity android:name=".Settings_activity" />
        <activity android:name=".HowToPlay"></activity>
    </application>

</manifest>